using System;
					
public class ProgrammingQ3
{
	public static void Main()
	{
		int firstValue;
		int secondValue;
		Console.Write("Please give a number as 1st variable:  ");
		firstValue = int.Parse(Console.ReadLine());
		Console.Write("Please give a number as 2nd variable:  ");
		secondValue = int.Parse(Console.ReadLine());
		
		firstValue = firstValue + secondValue;
		secondValue = firstValue - secondValue;
		firstValue = firstValue - secondValue;
		
		Console.WriteLine("The value of the 1st variable: " + firstValue);
		Console.WriteLine("The value of the 2nd variable: " + secondValue);
	}
}
